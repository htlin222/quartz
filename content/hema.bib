  title = {Double-Refractory {{Hodgkin}} Lymphoma: Tackling Relapse after Brentuximab Vedotin and Checkpoint Inhibitors},
  shorttitle = {Double-Refractory {{Hodgkin}} Lymphoma},
  author = {Epperla, Narendranath and Hamadani, Mehdi},
  date = {2021-12-10},
