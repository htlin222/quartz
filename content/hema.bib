}

@article{roschewski2021car,
  title = {{{CAR T-cell}} Therapy for Large {{B-cell}} Lymphoma - Who, When, and How?},
  author = {Roschewski, Mark and Longo, Dan L and Wilson, Wyndham H},
  date = {2021},
  journaltitle = {New England Journal of Medicine},
  shortjournal = {N. Engl. J. Med.},
  volume = {386},
  number = {7},
  pages = {692--696},
  doi = {10.1056/nejme2118899},
  abstract = {Large B-cell lymphoma is a spectrum of aggressive B-cell cancers with broad genetic and clinical heterogeneity.1 First-line chemotherapy cures most patients, but those with relapsed or refractory disease usually die of lymphoma. Salvage chemotherapy followed by autologous stem-cell transplantation (ASCT) is the standard second-line approach to large B-cell lymphoma and cures up to 30 to 40\% of eligible patients, but it is restricted to younger, fit patients and is relatively ineffective in chemotherapy-refractory disease.2 Chimeric antigen receptor (CAR) T-cell therapies that target CD19 have advanced the treatment of multiply relapsed large B-cell lymphoma and showed promising rates of durable remission . . .},
  langid = {english}
}

@article{roth_sutimlimab_2022,
  title = {Sutimlimab in Patients with Cold Agglutinin Disease: Results of the Randomized Placebo-Controlled Phase 3 {{CADENZA}} Trial},
