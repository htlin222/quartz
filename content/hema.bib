@article{huguet_intensified_2018,
  ids = {huguet2018intensified},
  title = {Intensified Therapy of Acute Lymphoblastic Leukemia in Adults: Report of the Randomized {{GRAALL-2005}} Clinical Trial},
  shorttitle = {Intensified {{Therapy}} of {{Acute Lymphoblastic Leukemia}} in {{Adults}}},
  author = {Huguet, Fran√ßoise and Chevret, Sylvie and Leguay, Thibaut and Thomas, Xavier and Boissel, Nicolas and Escoffre-Barbe, Martine and Chevallier, Patrice and Hunault, Mathilde and Vey, Norbert and Bonmati, Caroline and Lepretre, St√©phane and Marolleau, Jean-Pierre and Pabst, Thomas and Rousselot, Philippe and Buzyn, Agn√®s and Cahn, Jean-Yves and Lh√©ritier, V√©ronique and B√©n√©, Marie C. and Asnafi, Vahid and Delabesse, Eric and Macintyre, Elizabeth and Chalandon, Yves and Ifrah, Norbert and Dombret, Herv√© and Group of Research on Adult ALL (GRAALL)},
