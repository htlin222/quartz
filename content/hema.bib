@article{daw_risk_2020,
  title = {Risk and {{Response Adapted Treatment Guidelines}} for {{Managing First Relapsed}} and {{Refractory Classical Hodgkin Lymphoma}} in {{Children}} and {{Young People}}. {{Recommendations}} from the {{EuroNet Pediatric Hodgkin Lymphoma Group}}.},
