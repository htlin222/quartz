  author = {Berentsen, Sigbj√∏rn and Randen, Ulla and Oksman, Markku and Birgens, Henrik and Tvedt, Tor Henrik Anderson and Dalgaard, Jakob and Galteland, Eivind and Hauk√•s, Einar and Brudevold, Robert and S√∏rb√∏, Jon Hjalmar and N√¶ss, Inger Anne and Malecka, Agnieszka and Tj√∏nnfjord, Geir E.},
  date = {2017-07-27},
  journaltitle = {Blood},
