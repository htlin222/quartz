  file = {/Users/htlin/Documents/Journals/Blood/Min et al. - 2022 - Geriatric assessment predicts nonfatal toxicities .pdf;/Users/htlin/Zotero/storage/TF9T8FJD/Geriatric-assessment-predicts-nonfatal-toxicities.html}
}

