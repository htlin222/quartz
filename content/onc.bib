  author = {Loibl, S. and Schneeweiss, A. and Huober, J. and Braun, M. and Rey, J. and Blohmer, J. -U. and Furlanetto, J. and Zahm, D. -M. and Hanusch, C. and Thomalla, J. and Jackisch, C. and Staib, P. and Link, T. and Rhiem, K. and Solbach, C. and Fasching, P. A. and Nekljudova, V. and Denkert, C. and Untch, M.},
