@article{shah_zolbetuximab_2023,
  title = {Zolbetuximab plus {{CAPOX}} in {{CLDN18}}.2-Positive Gastric or Gastroesophageal Junction Adenocarcinoma: The Randomized, Phase 3 {{GLOW}} Trial},
