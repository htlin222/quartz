  author = {Rodriquenz, Maria Grazia and Ciardiello, Davide and Latiano, Tiziana Pia and Maiorano, Brigida Anna and Martinelli, Erika and Silvestris, Nicola and Ciardiello, Fortunato and Maiello, Evaristo},
