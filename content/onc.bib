  langid = {english},
  keywords = {BRAF,colorectal cancer,KRAS,metastases,microsatellite instability,targeted therapy},
  file = {/Users/mac/Documents/Journals/_/Ruff and Pawlik - 2023 - A Review of Translational Research for Targeted Th.pdf}
}

